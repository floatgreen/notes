# 12
select * from nobel where winner = 'EUGENE O''NEILL'

# 13
select winner, yr, subject
 from nobel 
where winner LIKE 'Sir%'
order by yr DESC, winner

# 14
SELECT winner, subject 
  FROM nobel
 WHERE yr=1984
 ORDER BY subject IN ('Physics','Chemistry'), subject,winner
 
